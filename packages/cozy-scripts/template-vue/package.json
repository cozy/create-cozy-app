{
  "name": "cozy-scripts-template-vue",
  "version": "0.1.0",
  "scripts": {
    "tx": "tx pull --all || true",
    "lint": "yarn lint:js && yarn lint:styles",
    "lint:js": "eslint '{src,test}/**/*.{js,vue}'",
    "lint:styles": "stylint src/styles --config ./.stylintrc",
    "prebuild": "yarn lint",
    "build": "yarn build:browser",
    "build:browser": "cozy-scripts build --vue --browser",
    "build:mobile": "cozy-scripts build --vue --mobile",
    "watch": "yarn watch:browser",
    "watch:browser": "cozy-scripts watch --vue --browser",
    "watch:mobile": "cozy-scripts watch --vue --mobile",
    "standalone": "yarn standalone:browser",
    "standalone:browser": "cozy-scripts standalone --vue --hot --browser",
    "standalone:mobile": "cozy-scripts standalone --vue --hot --mobile",
    "deploy": "git-directory-deploy --directory build/ --branch ${DEPLOY_BRANCH:-build} --repo=${DEPLOY_REPOSITORY:-https://$GITHUB_TOKEN@github.com/<USERNAME_GH>/<SLUG_GH>.git}",
    "pretest": "yarn lint",
    "test": "cozy-scripts test --verbose --coverage",
    "stack:docker": "docker run --rm -it -p 8080:8080 -p 5984:5984 -v \"$(pwd)/build\":/data/cozy-app/app cozy/cozy-app-dev",
    "cozyPublish": "git fetch origin ${DEPLOY_BRANCH:-build}:${DEPLOY_BRANCH:-build} && cozy-scripts publish --token $REGISTRY_TOKEN --build-commit $(git rev-parse ${DEPLOY_BRANCH:-build})"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/<USERNAME_GH>/<SLUG_GH>.git"
  },
  "author": "",
  "contributors": [],
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/<USERNAME_GH>/<SLUG_GH>/issues"
  },
  "homepage": "https://github.com/<USERNAME_GH>/<SLUG_GH>#readme",
  "devDependencies": {
    "@vue/test-utils": "1.0.0-beta.25",
    "babel-polyfill": "6.26.0",
    "babel-preset-cozy-app": "1.2.0",
    "eslint": "5.6.0",
    "eslint-plugin-prettier": "2.7.0",
    "eslint-plugin-vue": "4.7.1",
    "git-directory-deploy": "1.5.1",
    "jest-serializer-vue": "2.0.2",
    "prettier": "1.14.3",
    "stylint": "1.5.9",
    "vue-loader": "15.4.2",
    "vue-template-compiler": "2.5.17"
  },
  "dependencies": {
    "cozy-bar": "6.1.2",
    "cozy-client-js": "0.13.0",
    "cozy-ui": "11.2.1",
    "date-fns": "1.29.0",
    "eslint-config-cozy-app": "1.1.3",
    "node-polyglot": "2.3.0",
    "vue": "2.5.17",
    "vue-router": "3.0.1",
    "vuex": "3.0.1"
  }
}
